generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Snap {
  id        String   @id @default(uuid())
  content   String
  createdAt DateTime @default(now())
  userName  String
  // privado/publico
  // hashtags?
  // etiquetas?
}
